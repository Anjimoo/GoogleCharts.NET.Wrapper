<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Anton "AnJimoo" Ruvinsky</Authors>
    <Company></Company>
    <Description>Google Charts Wrapper for .NET ASP.NET projects.
</Description>
    <Version>1.0.7</Version>
